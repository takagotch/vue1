<html>
//1
<div id="app">
  <p>{{ msg }}</p>
</div>

//2
<div id="app">
<p v-if="visible()">{{ msg }}</p>
</div>

//3
<div>
<ol>
<li v-for="item in items">
{{ item.name }}
</li>
</ol>
</div>

<script>
//1
var app = new Vue({
  el: '#app',
  data: {
    message: 'Hello!'
  }
})

//2
var app = new Vue({
  el: '#app',
  data: {
    msg: 'Hello!'
  },
  methods: {
    visible: function() {
      return true;
    }
  }
});

//3
var app = new Vue({
  el: '#app',
  data: {
    items: [
      { name: 'tk'},
      { name: 'taka'}
      { name: 'tky'}
    ]
  }
});

//4

</script>
</html>

